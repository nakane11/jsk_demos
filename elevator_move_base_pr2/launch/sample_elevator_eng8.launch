<launch>
  <arg name="ROBOT" default="sim" />
  <param name="/use_sim_time" value="true"/>

  <node name="rosbag_play"
        pkg="rosbag" type="play"
        args="/home/nakane/20221026.bag --clock --loop"/>

  <node pkg="jsk_interactive_marker" name="camera_info_publisher_with_yaml" type="camera_info_publisher" output="screen">
    <remap from="~camera_info" to="/narrow_stereo/left/camera_info"/>
    <rosparam subst_value="true">
      yaml_filename: $(find elevator_move_base_pr2)/config/narrow_stereo_left_camera_info.yaml
      frame_id: narrow_stereo_optical_frame
    </rosparam>
  </node>

  <include file="$(find elevator_move_base_pr2)/launch/elevator_move_base_eng8.launch">
    <arg name="ROBOT" value="$(arg ROBOT)"/>
  </include>

</launch>
